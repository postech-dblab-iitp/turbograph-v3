// order by
MATCH (n:Person) RETURN n._id, n.id, n.firstName, n.lastName ORDER BY n.lastName DESC;
MATCH (n:Person) RETURN n._id, n.id, n.firstName, n.lastName ORDER BY n.firstName ASC, n.lastName DESC;
// group by
MATCH (n:Person) RETURN count(n);
MATCH (n:Person) RETURN DISTINCT count(n);
MATCH (n:Person) RETURN sum(n);