MATCH (v0)-[:`http://schema.org/author`]->(v1), (v0)-[:`http://schema.org/author`]->(v2), (v2)-[:`http://schema.org/worksFor`]->(v3), (v2)-[:`http://www.w3.org/2002/07/owl#sameAs`]->(v4), (v2)-[:`http://schema.org/birthPlace`]->(v5) WHERE v0.`http://schema.org/url` IS NOT NULL AND v0.`http://schema.org/alternateName` IS NOT NULL AND v3.`http://schema.org/url` IS NOT NULL RETURN v0.`http://schema.org/image` AS `v0_image`, v1.`http://www.w3.org/2000/01/rdf-schema#label` AS `v1_rdf-schema#label`, v2.`http://schema.org/mainEntityOfPage` AS `v2_mainEntityOfPage`, v3.`http://schema.org/dateCreated` AS `v3_dateCreated`, v5.`http://schema.org/dissolutionDate` AS `v5_dissolutionDate`, COUNT(v3.`http://schema.org/dateCreated`) AS `v3_dateCreated_count` ORDER BY `v2_mainEntityOfPage`