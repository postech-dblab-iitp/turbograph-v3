MATCH (v0)-[:`http://schema.org/location`]->(v1), (v1)-[:`http://yago-knowledge.org/resource/capital`]->(v2), (v2)-[:`http://schema.org/location`]->(v3), (v3)-[:`http://schema.org/location`]->(v4), (v4)-[:`http://yago-knowledge.org/resource/replaces`]->(v5) WHERE v3.`http://yago-knowledge.org/resource/elevation` IS NOT NULL AND v3.`http://schema.org/mainEntityOfPage` IS NOT NULL RETURN v0.`http://www.w3.org/2000/01/rdf-schema#comment` AS `v0_rdf-schema#comment`, v1.`http://schema.org/image` AS `v1_image`, v2.`http://schema.org/alternateName` AS `v2_alternateName`, v3.`http://yago-knowledge.org/resource/populationNumber` AS `v3_populationNumber`, v4.`http://schema.org/geo` AS `v4_geo`, v5.`http://schema.org/geo` AS `v5_geo`, COUNT(v5.`http://schema.org/geo`) AS `v5_geo_count` ORDER BY `v1_image`